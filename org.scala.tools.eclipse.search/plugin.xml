<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="example.actionSet"
            label="Sample Action Set"
            visible="true">
         <menu
               id="semanticSearchMenu"
               label="Semantic &amp;Search">
            <separator
                  name="sampleGroup">
            </separator>
         </menu>
         <action
               class="org.scala.tools.eclipse.search.actions.FindReferencesToMethodAction"
               id="org.scala.tools.eclipse.search.actions.FindReferencesToMethodAction"
               label="&amp;Find References To Method"
               menubarPath="semanticSearchMenu/sampleGroup">
         </action>
      </actionSet>
   </extension>
</plugin>
